class Cat {
  init(name) {
    this.name = name;
  }

  say_meow() {
    return "meow";
  }
}

var kitten = Cat("Mochi");

print kitten;
print kitten.name;

print kitten.say_meow();

class Cake {
  taste() {
    var adjective = "delicious";
    print "The " + this.flavor + " cake is " + adjective + "!";
  }
}

var cake = Cake();
cake.flavor = "German chocolate";
cake.taste();

class Vehicle {
  move() {
    print "go!";
  }
}

class Car < Vehicle {}

var car = Car();
car.move();

